    "def get_data_files(sub_dir: str, sub_dirs: str, file_extension: str='pgm') -> list:\n",
    "    data = []\n",
    "    for sub_dir in sub_dirs:\n",
    "        data_files = glob.glob(f\"{dataset_target_dir}/{sub_dir}/*.{file_extension}\")\n",
    "        data.extend(data_files)\n",
    "    return data"
   ]
